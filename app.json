{
  "expo": { 
    "name": "IN42",
    "slug": "IN42",
    "version": "0.3.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "in42",
    "plugins": [
      ["expo-build-properties", {
        "android": {
          "usesCleartextTraffic": true
        },
        "ios": {
        }
      }]],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mrm0lten.IN42"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mrm0lten.IN42",
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3db51579-3df9-4f7d-b340-c3a0aa40600f"
      }
    }
  }
